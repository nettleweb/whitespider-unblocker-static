{
	"manifest_version": 2,
	"name": "ChromeHack",
	"short_name": "ChromeHack",
	"description": "Site unblocker",
	"version": "0.1.0",
	"display": "fullscreen",
	"orientation": "any",
	"background_color": "#ffffff",
	"theme_color": "#ffffff",
	"start_url": "index.html",
	"scope": "/",
	"icons": [
		{
			"src": "res/logo64.png",
			"sizes": "64x64",
			"type": "image/png"
		},
		{
			"src": "res/logo128.png",
			"sizes": "128x128",
			"type": "image/png"
		},
		{
			"src": "res/logo256.png",
			"sizes": "256x256",
			"type": "image/png"
		},
		{
			"src": "res/logo512.png",
			"sizes": "512x512",
			"type": "image/png"
		}
	]
}